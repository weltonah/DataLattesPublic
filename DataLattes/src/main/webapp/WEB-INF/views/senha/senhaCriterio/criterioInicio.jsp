<div class=" col-4">
 <h2>Criterios Gerais</h2>
</div>
<div id="accordionInicio">
 <div class="card " style="margin-bottom: 10px; border-width: 0px;">
  <div class="card-header" id="headingOne"
   style="padding: 0px; border-width: 0px;">
   <h5 class="mb-0">
    <button class="btn btn-outline-secondary" id="buttonOne"
     style="width: 100%;" data-toggle="collapse"
     data-target="#collapseOne" aria-expanded="true"
     aria-controls="collapseOne">
     <div class="row">
      <div class="col-2">Anos de analise</div>
      <div class="col-1 offset-9 ">
       <i class="" id="iconOne"></i>
      </div>
     </div>
    </button>
   </h5>
  </div>
  <div id="collapseOne" class="collapse show"
   aria-labelledby="headingOne" data-parent="#accordionInicio">
   <div class="card-body row ">
    <div class=" col-3">
     <div class="input-group mb-3">
      <div class="input-group-prepend">
       <span class="input-group-text" id="basic-addon1">Ano
        inicial</span>
      </div>
      <input type="number" class="form-control AnoCriterio"
       id="AnoInicio" name="AnoInicio" placeholder="anos" min="1900"
       value="1950">
     </div>
    </div>
    <div class=" col-3 ">
     <div class="input-group mb-3">
      <div class="input-group-prepend">
       <span class="input-group-text" id="basic-addon1">Ano final</span>
      </div>
      <input type="number" class="form-control AnoCriterio" id="AnoFim"
       value="" name="AnoFim" placeholder="anos">
     </div>
    </div>
   </div>
  </div>
 </div>
</div>